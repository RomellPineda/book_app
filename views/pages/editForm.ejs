<h1>You're in Edit Form</h1>
<form action="/save-book" method="POST">
  <input type="text" name="image_url" value="<%= book.image_url %>" required>
  <input type="text" name="title" value="<%= book.title %>" required>
  <input type="text" name="author" value="<%= book.author %>" required>
  <input type="text" name="isbn" value="<%= book.isbn %>" required>
  <input type="text" name="categorie" value="<%= book.categorie %>" required>
  <input type="text" name="summary" value="<%= book.summary %>" required>
  <input type="submit" value="Add to Database">
</form>