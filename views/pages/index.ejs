<main>
  <h3 class="center"><span id="count"><%=  booksArray.length %></span> Books</h3>
  <div class="content">
    <% booksArray.forEach(book => { %>
    <% if (book.image_url) { %>
    <img src="<%= book.image_url %>" />
    <% } %>
    <h2><%= book.title %> </h2>
    <h3><%= book.author %> </h3>
    <h4><%= book.isbn %> </h4>
    <h3><%= book.categorie %> </h3>
    <p><%= book.summary %> </p>
    <% }) %>
  </div>
</main>